-- PROJECT.CATEGORY definition

CREATE TABLE "PROJECT"."CATEGORY" 
   (	"CNO" NUMBER(20,0) NOT NULL ENABLE, 
	"CTG" VARCHAR2(100) NOT NULL ENABLE, 
	"CTG_ENG" VARCHAR2(100) NOT NULL ENABLE, 
	"DCNO" NUMBER(20,0) NOT NULL ENABLE, 
	"DCTG" VARCHAR2(100) NOT NULL ENABLE, 
	"DCTG_ENG" VARCHAR2(100) NOT NULL ENABLE, 
	"QUE" VARCHAR2(100) NOT NULL ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

COMMENT ON COLUMN "PROJECT"."CATEGORY"."CNO" IS '"카테고리 번호"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."CTG" IS '"카테고리"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."CTG_ENG" IS '"카테고리 영문"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."DCNO" IS '"세부카테고리 번호"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."DCTG" IS '"세부카테고리"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."DCTG_ENG" IS '"세부카테고리 영문"';
   COMMENT ON COLUMN "PROJECT"."CATEGORY"."QUE" IS '"질문"';
   
   
   -- PROJECT.CHAT definition

CREATE TABLE "PROJECT"."CHAT" 
   (	"FROM_MNO" NUMBER(20,0) NOT NULL ENABLE, 
	"GETCHAT" VARCHAR2(100) NOT NULL ENABLE, 
	"SENDCHAT" VARCHAR2(100) NOT NULL ENABLE, 
