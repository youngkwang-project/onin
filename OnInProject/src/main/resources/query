   -- PROJECT.PRO_MEMBER definition

CREATE TABLE "PROJECT"."PRO_MEMBER" 
   (	"MNO" NUMBER(20,0) NOT NULL ENABLE, 
	"PRFILE_IMG" VARCHAR2(100) NOT NULL ENABLE, 
	"INTRO" VARCHAR2(100) NOT NULL ENABLE, 
	"PROFILE" VARCHAR2(100) NOT NULL ENABLE, 
	"CAREER" VARCHAR2(100) NOT NULL ENABLE, 
	"SKILL" VARCHAR2(100) NOT NULL ENABLE, 
	"CAREER_FILE" VARCHAR2(100) NOT NULL ENABLE, 
	"PORTFOLIO_TITLE" VARCHAR2(100) NOT NULL ENABLE, 
	"PORTFOLIO_LINK" VARCHAR2(100) NOT NULL ENABLE, 
	"PORTFOLIO_EXPLAIN" VARCHAR2(100) NOT NULL ENABLE, 
	"PORTFOLIO_IMG" VARCHAR2(100) NOT NULL ENABLE, 
	"ADD" VARCHAR2(100) NOT NULL ENABLE, 
	"ADD_DETAIL" VARCHAR2(100) NOT NULL ENABLE, 
	"ADD_POST" VARCHAR2(100) NOT NULL ENABLE, 
	"BANK" VARCHAR2(100) NOT NULL ENABLE, 
	"ACC" VARCHAR2(100) NOT NULL ENABLE, 
	"PROFILE_OPEN" VARCHAR2(100) NOT NULL ENABLE, 
	"MONEY" NUMBER(20,0) NOT NULL ENABLE, 
	"PRO_OPEN" VARCHAR2(100) NOT NULL ENABLE, 
	"CNO" NUMBER(20,0) NOT NULL ENABLE, 
	"DCNO" NUMBER(20,0) NOT NULL ENABLE, 
	"PRO_MNO" NUMBER(20,0) NOT NULL ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."MNO" IS '회원번호';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PRFILE_IMG" IS '프로필사진';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."INTRO" IS '한줄프로필';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PROFILE" IS '상세소개';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."CAREER" IS '경력';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."SKILL" IS '보유기술';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."CAREER_FILE" IS '경력,전공,자격,증빙자료';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PORTFOLIO_TITLE" IS '포트폴리오 제목';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PORTFOLIO_LINK" IS '미디어링크';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PORTFOLIO_EXPLAIN" IS '상세설명';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PORTFOLIO_IMG" IS '이미지파일';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."ADD" IS '주소';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."ADD_DETAIL" IS '세부주소';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."ADD_POST" IS '우편주소';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."BANK" IS '계좌은행';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."ACC" IS '계좌번호';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PROFILE_OPEN" IS '프로필 공개 여부';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."MONEY" IS '잔고';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PRO_OPEN" IS '임시공개';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."CNO" IS '카테고리번호';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."DCNO" IS '세부카테고리번호';
   COMMENT ON COLUMN "PROJECT"."PRO_MEMBER"."PRO_MNO" IS '전문가 회원번호';
   
   
